{
  "manifest_version": 2,
  "name": "Simple Video Autoplay Stopper",
  "description": "A very minimal extension to stop autoplaying videos on CNN",
  "version": "1.0",
  "permissions": [
    "tabs",
    "*://*.cnn.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.cnn.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "128": "icon.png"
  }
}
